<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_enabled="true">
		<shape>
			<corners android:radius="2dp"/>
			<solid android:color="#26AB28"/>
			<padding
				android:bottom="6dp"
				android:left="10dp"
				android:right="10dp"
				android:top="6dp"/>
		</shape>
	</item>
	<item android:state_enabled="false">
		<shape>
			<corners android:radius="2dp"/>
			<solid android:color="#2E6733"/>
			<padding
				android:bottom="6dp"
				android:left="10dp"
				android:right="10dp"
				android:top="6dp"/>
		</shape>
	</item>
</selector>